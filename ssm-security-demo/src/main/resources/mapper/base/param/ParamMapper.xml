<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.mapper.base.param.ParamMapper">
    <resultMap id="BaseResultMap" type="com.ssm.dto.base.param.Param">
        <id column="tid" jdbcType="INTEGER" property="tid"/>
        <result column="param_type" jdbcType="VARCHAR" property="paramType"/>
        <result column="param_type_name" jdbcType="VARCHAR" property="paramTypeName"/>
        <result column="param_name" jdbcType="VARCHAR" property="paramName"/>
        <result column="param_value" jdbcType="VARCHAR" property="paramValue"/>
        <result column="remark" jdbcType="VARCHAR" property="remark"/>
        <result column="oper_user" jdbcType="VARCHAR" property="operUser"/>
        <result column="last_update" jdbcType="TIMESTAMP" property="lastUpdate"/>
    </resultMap>
    <sql id="Base_Column_List">
        tid,
        param_type,
        param_type_name,
        param_name,
        param_value,
        remark,
        oper_user,
        last_update
    </sql>
    <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from pm_param
        where tid = #{tid,jdbcType=INTEGER}
    </select>
    <select id="getProjectName" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from pm_param
    </select>
    <select id="list" resultType="com.ssm.dto.base.param.Param">
        SELECT DISTINCT
            param_type      paramType,
            param_type_name paramTypeName
        FROM pm_param
    </select>
    <select id="paramDetail" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from pm_param t where t.param_type = #{paramType};
    </select>
    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        DELETE
        FROM pm_param
        WHERE tid = #{tid,jdbcType=INTEGER}
    </delete>
    <delete id="deletes">
        delete from pm_param WHERE tid in
        <foreach collection="list" item="tid" open="(" separator="," close=")">
            #{tid}
        </foreach>
    </delete>
    <insert id="insert" parameterType="com.ssm.dto.base.param.Param">
        INSERT INTO pm_param (tid, param_type, param_type_name,
                              param_name, param_value, remark,
                              oper_user, last_update)
        VALUES (#{tid,jdbcType=INTEGER}, #{paramType,jdbcType=VARCHAR}, #{paramTypeName,jdbcType=VARCHAR},
                #{paramName,jdbcType=VARCHAR}, #{paramValue,jdbcType=VARCHAR}, #{remark,jdbcType=VARCHAR},
                #{operUser,jdbcType=VARCHAR}, #{lastUpdate,jdbcType=TIMESTAMP})
    </insert>
    <insert id="insertSelective" parameterType="com.ssm.dto.base.param.Param">
        insert into pm_param
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="tid != null">
                tid,
            </if>
            <if test="paramType != null">
                param_type,
            </if>
            <if test="paramTypeName != null">
                param_type_name,
            </if>
            <if test="paramName != null">
                param_name,
            </if>
            <if test="paramValue != null">
                param_value,
            </if>
            <if test="remark != null">
                remark,
            </if>
            <if test="operUser != null">
                oper_user,
            </if>
            <if test="lastUpdate != null">
                last_update,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="tid != null">
                #{tid,jdbcType=INTEGER},
            </if>
            <if test="paramType != null">
                #{paramType,jdbcType=VARCHAR},
            </if>
            <if test="paramTypeName != null">
                #{paramTypeName,jdbcType=VARCHAR},
            </if>
            <if test="paramName != null">
                #{paramName,jdbcType=VARCHAR},
            </if>
            <if test="paramValue != null">
                #{paramValue,jdbcType=VARCHAR},
            </if>
            <if test="remark != null">
                #{remark,jdbcType=VARCHAR},
            </if>
            <if test="operUser != null">
                #{operUser,jdbcType=VARCHAR},
            </if>
            <if test="lastUpdate != null">
                #{lastUpdate,jdbcType=TIMESTAMP},
            </if>
        </trim>
    </insert>
    <update id="updateByPrimaryKeySelective" parameterType="com.ssm.dto.base.param.Param">
        update pm_param
        <set>
            <if test="paramType != null">
                param_type = #{paramType,jdbcType=VARCHAR},
            </if>
            <if test="paramTypeName != null">
                param_type_name = #{paramTypeName,jdbcType=VARCHAR},
            </if>
            <if test="paramName != null">
                param_name = #{paramName,jdbcType=VARCHAR},
            </if>
            <if test="paramValue != null">
                param_value = #{paramValue,jdbcType=VARCHAR},
            </if>
            <if test="remark != null">
                remark = #{remark,jdbcType=VARCHAR},
            </if>
            <if test="operUser != null">
                oper_user = #{operUser,jdbcType=VARCHAR},
            </if>
            <if test="lastUpdate != null">
                last_update = #{lastUpdate,jdbcType=TIMESTAMP},
            </if>
        </set>
        where tid = #{tid,jdbcType=INTEGER}
    </update>
    <update id="updateByPrimaryKey" parameterType="com.ssm.dto.base.param.Param">
        UPDATE pm_param
        SET param_type      = #{paramType,jdbcType=VARCHAR},
            param_type_name = #{paramTypeName,jdbcType=VARCHAR},
            param_name      = #{paramName,jdbcType=VARCHAR},
            param_value     = #{paramValue,jdbcType=VARCHAR},
            remark          = #{remark,jdbcType=VARCHAR},
            oper_user       = #{operUser,jdbcType=VARCHAR},
            last_update     = #{lastUpdate,jdbcType=TIMESTAMP}
        WHERE tid = #{tid,jdbcType=INTEGER}
    </update>
</mapper>